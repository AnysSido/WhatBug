@model WhatBug.WebUI.ViewComponents.UserSelectorComponentViewModel

@{
    <select asp-for="@Model.SelectedUserId" class="select2">
        @for (int i = 0; i < Model.Users.Count; i++)
        {
            <option value="@Model.Users[i].Id">@Model.Users[i].Username</option>
        }
    </select>
}
